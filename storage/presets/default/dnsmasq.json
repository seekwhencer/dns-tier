{
  "verbose": 2,
  "config": {
    "log-queries": true,
    "strict-order": true,
    "expand-hosts": true,
    "no-resolv": true,
    "no-hosts": true,
    "domain-needed": false,
    "bogus-priv": false,
    "server": [
      "192.168.178.1",
      "8.8.8.8"
    ],
    "interface": [
      "wlan0",
      "wlan1"
    ],
    "no-dhcp-interface": [
      "eth0"
    ],
    "dhcp-range": "192.168.180.100,192.168.180.150,24h",
    "dhcp-option": [
      "wlan0,option:dns-server,192.168.180.1",
      "wlan1,option:dns-server,192.168.180.2"
    ],
    "local": "/koffer/",
    "domain": "koffer",
    "listen-address": [
      "127.0.0.1",
      "192.168.180.1",
      "192.168.180.2"
    ],
    "bind-interfaces": true,
    "address": [
      "/koffer/192.168.180.1",
      "/api.koffer/192.168.180.1",
      "/git.koffer/192.168.180.1",
      "/npm.koffer/192.168.180.1",
      "/docker.koffer/192.168.180.1"
    ]
  },
  "dynamic": {
    "dhcp-leasefile": true,
    "conf-dir": false,
    "hosts-dir": false
  }
}
